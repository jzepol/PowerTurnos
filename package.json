{
  "name": "turnos-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "logs:clean": "node scripts/cleanup-logs.js",
    "logs:check": "powershell -Command \"Get-ChildItem -Path src -Recurse -Include *.ts,*.tsx | Select-String 'console\\.log' | Measure-Object | Select-Object -ExpandProperty Count\"",
    "logs:fix-imports": "node scripts/fix-logger-imports.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.7",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.5.6",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}
